{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/crow",
                "${workspaceFolder}/crow/**",
                "${workspaceFolder}/crow/include",
                "${workspaceFolder}/crow/include/**",
                "${workspaceFolder}/jwt",
                "${workspaceFolder}/jwt/**",
                "${workspaceFolder}/boost/include",
                "${workspaceFolder}/boost/include/**",
                "/usr/local/include/mongocxx/v_noabi",
                "/usr/local/include/mongocxx/v_noabi/**",
                "/usr/local/include/bsoncxx/v_noabi",
                "/usr/local/include/bsoncxx/v_noabi/**",
                "/usr/local/include/bsoncxx/v_noabi/bsoncxx/third_party/mnmlstc",
                "/usr/local/include/bsoncxx/v_noabi/bsoncxx/third_party/mnmlstc/**",
                "/usr/include/x86_64-linux-gnu/c++/11",
                "/usr/include/x86_64-linux-gnu/c++/11/**",
                "/usr/include/c++/11/"
            ],
            "defines": [
                "-DCROW_USE_LOCALTIMEZONE",
                "-DCROW_FANCY_LOG"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}