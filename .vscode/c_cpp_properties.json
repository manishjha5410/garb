{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/crow",
                "${workspaceFolder}/crow/**",
                "${workspaceFolder}/crow/include",
                "${workspaceFolder}/crow/include/**",
                "${workspaceFolder}/jwt",
                "${workspaceFolder}/jwt/**",
                "${workspaceFolder}/boost/include",
                "${workspaceFolder}/boost/include/**",
                "/usr/local/include/mongocxx/v_noabi",
                "/usr/local/include/mongocxx/v_noabi/**",
                "/usr/local/include/bsoncxx/v_noabi",
                "/usr/local/include/bsoncxx/v_noabi/**",
                "/usr/local/include/bsoncxx/v_noabi/bsoncxx/third_party/mnmlstc",
                "/usr/local/include/bsoncxx/v_noabi/bsoncxx/third_party/mnmlstc/**"
            ],
            "defines": ["-DCROW_USE_LOCALTIMEZONE","-DCROW_FANCY_LOG"],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}